<ul>
    <% creaturesData.forEach((creature, index) => { %>
 <li>
    <%= creature.name %> is a <%= creature.type %>
    <a href='/creatures/edit/<%= index %>'>Edit creature</a>

    <form action='/creatures/<%= index %>/?_method=DELETE' method='POST'>
        <input type='submit' value="Delete Creature" />
    </form>
</li>
<% }) %>
</ul>